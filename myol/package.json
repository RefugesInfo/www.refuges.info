{
  "name": "@myol/myol",
  "version": "1.1.2.dev",
  "description": "This package adds many features to Openlayer https://openlayers.org/",
  "author": "Dominique Cavailhez",
  "homepage": "https://github.com/Dominique92/myol#readme",
  "license": "MIT",
  "main": "dist/myol.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dominique92/myol.git"
  },
  "bugs": {
    "url": "https://github.com/Dominique92/myol/issues"
  },
  "keywords": [
    "Openlayers"
  ],
  "files": [
    "src",
    "dist"
  ],
  "type": "module",
  "scripts": {
    "build": "rollup -c build/config.js",
    "lint": "npx eslint src/*.js src/*/*.js",
    "beautify": "js-beautify -s=2 build/*.js gps/*/*.*s examples/*.html examples/ressources/* examples/wri/*.h* examples/wri/vues/*.js src/*.*s src/control/* src/layer/* package.json"
  },
  "dependencies": {
    "ol": "8.2.0",
    "proj4": "2.9.0",
    "@myol/geocoder": "4.2.16"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "25.0.4",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.2.1",
    "@rollup/plugin-terser": "0.4.3",
    "eslint": "8.49.0",
    "js-beautify": "1.14.9",
    "rollup": "3.29.2",
    "rollup-plugin-import-css": "3.3.3"
  }
