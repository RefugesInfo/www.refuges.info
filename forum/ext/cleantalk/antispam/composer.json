{
    "name": "cleantalk/antispam",
    "type": "phpbb-extension",
    "description": "Antispam by CleanTalk",
    "homepage": "https://cleantalk.org",
    "version": "4.7",
    "time": "2016-08-18",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Alexey Znaev",
            "email": "welcome@cleantalk.org",
            "homepage": "https://cleantalk.org",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phpbb/phpbb": ">=3.1.0-RC2,<3.2.0@dev"
    },
    "require-dev": {
        "phpbb/epv": "dev-master"
    },
    "extra": {
        "display-name": "Antispam by CleanTalk",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.0-RC2,<3.2.0@dev"
